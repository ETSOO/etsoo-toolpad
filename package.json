{
  "name": "@etsoo/toolpad",
  "version": "1.0.11",
  "author": "ETSOO",
  "description": "Dashboard framework extention based on Toolpad Core",
  "main": "build/index.js",
  "module": "build/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./nextjs": {
      "types": "./build/nextjs/index.d.ts",
      "import": "./build/nextjs/index.js"
    },
    "./react-router-dom": {
      "types": "./build/react-router-dom/index.d.ts",
      "import": "./build/react-router-dom/index.js"
    }
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "mui",
    "toolpad",
    "dashboard",
    "Etsoo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ETSOO/etsoo-toolpad.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ETSOO/etsoo-toolpad/issues"
  },
  "homepage": "https://github.com/ETSOO/etsoo-toolpad#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "testb": "vitest --browser.enabled"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/utils": "6.3.1",
    "invariant": "2.2.4",
    "path-to-regexp": "6.3.0",
    "react": "^18.3.1"
  },
  "overrides": {
    "react": "$react"
  },
  "devDependencies": {
    "@mui/icons-material": "6.3.1",
    "@mui/material": "6.3.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/invariant": "2.2.37",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/browser": "2.1.8",
    "jsdom": "^25.0.1",
    "next": "^15.0.3",
    "next-router-mock": "^0.9.13",
    "playwright": "^1.49.1",
    "react-router-dom": "7.1.1",
    "vitest": "2.1.8"
  }
}
